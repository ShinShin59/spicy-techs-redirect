      "descOverride": {
        "descOverride": [
          {
            "_attrs": {
              "faction": "Vernius"
            },
            "desc": {
              "value": "Human resources remain a very important asset in the struggle for Arrakis. [Manpower] is used to train soldiers and recover damaged units' [Life]."
            }
          }
        ]
      }
    }
  },
  "FuelCell": {
    "texts": {
      "name": {
        "value": "Fuel Cell"
      },
      "desc": {
        "value": "[FuelCell]s are used to operate vehicles and drones far away from encampement, as well as to power heavy industries."
